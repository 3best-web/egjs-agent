{
	"tags": {
		"allowUnknownTags" : true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src", "README.md"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
		"template": "node_modules/egjs-jsdoc-template",
		"destination": "./doc/"
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/ko", 
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/group",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/codepen",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/support"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "egjs-agent",
		"disqus": "egjs",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "egjs-agent - API",
			"description": "Returns the User-Agent information.",
			"keyword": "egjs, agent"
		},
		"linenums": true,
		"link": {
			"canonical": ""
		}
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples", "ko"]
	}
}
